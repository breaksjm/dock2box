#!ipxe

:begin
prompt --key 0x02 --timeout 2000 "Press Ctrl-B for the iPXE shell..." || goto no_shell
shell
exit

:dock2box
dhcp || goto begin
chain http://%%server-name%%:8080/pxe/${netX/mac}?ipv4=${netX/ip}&netmask=${netX/netmask}&gateway=${netX/gateway}&serial=${serial:uristring} || goto dock2box
